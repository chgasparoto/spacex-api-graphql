{
  "name": "spacex-graphql-server",
  "version": "1.0.0",
  "description": "A simple graphql server for spacex data",
  "main": "src/app.js",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon src/app.js",
    "test": "NODE_ENV=test jest -i --forceExit --detectOpenHandles",
    "lint": "eslint \"**/*.js\"",
    "start": "node src/app.js"
  },
  "engines": {
    "node": "8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chgasparoto/spacex-api-graphql.git"
  },
  "keywords": [
    "spacex",
    "api",
    "graphql"
  ],
  "author": "Cleber Gasparoto <chgasparoto@gmail.com> (https://github.com/chgasparoto)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chgasparoto/spacex-api-graphql/issues"
  },
  "homepage": "https://github.com/chgasparoto/spacex-api-graphql#readme",
  "devDependencies": {
    "ajv": "6.5.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "graphql-tester": "0.0.5",
    "jest": "23.6.0",
    "jest-chain": "1.0.3",
    "jest-extended": "0.10.0",
    "jest-tobetype": "1.2.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "axios": "0.18.0",
    "cors": "2.8.4",
    "dataloader": "1.4.0",
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "graphql": "14.0.2"
  },
  "jest": {
    "bail": true,
    "setupTestFrameworkScriptFile": "./testSetup.js",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ]
  }
}
